{
  "name": "git-browse",
  "version": "0.0.8beta",
  "description": "git-browse opens a Github repository with browser.",
  "keywords": [
    "git",
    "CLI",
    "terminal"
  ],
  "bin": {
    "git-browse": "index.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "$(npm bin)/mocha ./test/*.unit.js"
  },
  "author": "KamataRyo <mugil.cephalus+github.com@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamataRyo/git-browse.git"
  },
  "bugs": {
    "url": "https://github.com/KamataRyo/git-browse/issues"
  },
  "homepage": "https://github.com/KamataRyo/git-browse#readme",
  "config": {
    "protocol": "https",
    "host": "github.com"
  },
  "devDependencies": {
    "mocha": "^3.0.2",
    "should": "^11.1.0"
  },
  "dependencies": {
    "git-browse-github-search": "0.0.1"
  }
}
